    "\n",
    "#lower case db\n",
    "df_no_col_low = df_no_col.applymap(lambda x: x.lower() if type(x) == str else x)\n",
    "\n",
    "#create key value pairs from data_name and method_db\n",
    "discovered = {}\n",
    "for row in df_no_col_low.iterrows():\n",
    "    key = df_no_col_low.loc[row[0]][15]\n",
    "    value = df_no_col_low.loc[row[0]][9]\n",
    "    if pd.isna(key):\n",
    "        pass\n",
    "    else:\n",
    "        if key not in discovered:\n",
    "            discovered[key] = [value]\n",
    "        else:\n",
    "            if value not in discovered[key]:\n",
    "                discovered[key].append(value)\n",
    "            else:\n",
    "                pass\n",
    "\n",
    "#create key value pairs from data_name and best_match\n",
    "for row in df_no_col_low.iterrows():\n",
    "    key = df_no_col_low.loc[row[0]][15]\n",
    "    value = df_no_col_low.loc[row[0]][14]\n",
    "    if pd.isna(key):\n",
    "        pass\n",
    "    else:\n",
    "        if key not in discovered:\n",
    "            discovered[key] = [value]\n",
