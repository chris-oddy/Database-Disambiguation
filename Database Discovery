    "#find and remove values that apply to several keys\n",
    "all_vals = []\n",
    "for key, value in discovered.items():\n",
    "    if \"and\" not in key:\n",
    "        for item in value:\n",
    "            all_vals.append(item)\n",
    "        \n",
    "seen = set()\n",
    "doubles = []\n",
    "for item in all_vals:\n",
    "    if item not in seen:\n",
    "        seen.add(item)\n",
    "    else:\n",
    "        doubles.append(item)\n",
    "\n",
    "doubles = sorted(set(doubles))      \n",
    "print(doubles)\n",
    "\n",
    "for key, value in discovered.items():\n",
    "    for item in value:\n",
    "        if item in doubles:\n",
    "            print(\"removed:\"+item)\n",
    "            value.remove(item)"
   ]
