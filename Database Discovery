   "source": [
    "#collate discovered\n",
    "def collate_data(discovered, method, abstract):\n",
    "    if pd.isnull(discovered):\n",
    "        if pd.isnull(method):\n",
    "            if pd.isnull(abstract):\n",
    "                pass\n",
    "            return abstract\n",
    "        return method\n",
    "    return discovered\n",
    "\n",
    "df_discovered[\"discovered_db\"] = df_discovered[\"data_name\"]\n",
    "df_discovered[\"discovered_db\"] = df_discovered.apply(lambda x: collate_data(x[\"discovered_db\"], x[\"discovered_method\"], x[\"discovered_abstract\"]), axis=1)\n",
    "\n",
    "df_discovered.to_csv(\"C:\\\\Users\\\\chris\\\\Documents\\\\Job Documents\\\\Portfolio Evidence\\\\Research\\\\Health AI Tracking\\\\df_discovered.csv\")"
