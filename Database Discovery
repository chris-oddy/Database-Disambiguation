    "            else:\n",
    "                pass\n",
    "            \n",
    "#extract acronyms from key value pairs and add to values\n",
    "for key, value in discovered.items():\n",
    "    y = str(key)\n",
    "    if \"(\" in y:\n",
    "        brackets = y[y.find(\"(\")+1:y.find(\")\")]\n",
    "        if brackets not in value:\n",
    "            if len(brackets) < 20:\n",
    "                discovered[key].append(brackets)\n",
    "    for x in value:\n",
    "        y = str(x)\n",
    "        if \"(\" in y:\n",
    "            brackets = y[y.find(\"(\")+1:y.find(\")\")]\n",
    "            if brackets not in value:\n",
