   "source": [
    "#check method_db/abstract_db against discovered\n",
    "def data_name_extract(data_name, best_score, database):\n",
    "    if pd.isnull(data_name):\n",
    "        global number_analysed\n",
    "        number_analysed += 1\n",
    "        for key, value in discovered.items():\n",
    "            for item in value:\n",
    "                if fuzz.ratio(str(database), item) > 90:\n",
    "                    global count_discovered \n",
    "                    count_discovered += 1\n",
    "                    print(str(count_discovered)+\". \"+str(database)+\" matched with: '\"+str(item)+\"'. Now listed as '\"+str(key)+\"'.\")\n",
    "                    return key\n",
    "\n",
    "number_analysed = 0\n",
    "count_discovered = 0\n",
    "df_discovered = df\n",
    "df_discovered[\"discovered_method\"] = df_discovered.apply(lambda x: data_name_extract(x[\"data_name\"], x[\"best_score\"], x[\"method_db\"]), axis=1)\n",
    "df_discovered[\"discovered_abstract\"] = df_discovered.apply(lambda x: data_name_extract(x[\"data_name\"], x[\"best_score\"], x[\"abstract_db\"]), axis=1)\n",
    "\n",
    "print(\"\\n\"+\"Analysed \"+str(number_analysed)+\" databases\")\n",
    "print(\"Discovered \"+str(count_discovered)+\" Databases\")"
   ]
  },
